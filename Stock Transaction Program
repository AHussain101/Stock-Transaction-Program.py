shares= int(input("Number of Shares: "))
pricepershare= float(input("Price per Share: "))
cost= (float(shares * pricepershare))
print("Cost before commision: " + str(cost))
comRate= float(input("Commision Rate: "))
com= float(cost * comRate)
print("Commision: " + str(com))
totcost= float(com + cost)
print("Total Cost: " + str(totcost))
sprice= float(input("Sale Price: "))
salebc= float(shares * sprice)
print("Sale Before Commission: " + str(salebc))
salec = float(salebc * comRate)
print("Sale Commission: " + str(salec))
netp = float(salebc - salec)
print("Net Proceeds: " + str(netp))
netprof = float(netp - totcost)
print("Net Profit: " + "$" + str(netprof))

